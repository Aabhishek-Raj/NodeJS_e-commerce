<div class="container pt-5 mt-5">
    <div class="row">
        <table>
            <!-- Start Wishlist Table Head -->
            <thead >
                <tr>   
                    <th class="product_thumb ">S.No</th>
                    <th class="product_thumb ">Category List</th>
                    <th class="product_remove">Delete Category</th>
                </tr>
            </thead> <!-- End Cart Table Head -->
            <tbody>
                <!-- Start Wishlist Single Item-->
                <% category.forEach((category, index) => { %>    
                <tr>
                    <td class="product-price"><h4><%= index + 1 %></h4></td>
                    <td class="product-price"><h4><%=category.category %></h4></td>
                    <td class="product_remove "><a href="/admin/deletecategory/<%=category._id%>"><i class="btn btn-md btn-aqua">Delete</i></a>
                    </td>   
                </tr> <!-- End Wishlist Single Item-->
                <% }); %>
            </tbody>
        </table>
    </div>
</div>
   