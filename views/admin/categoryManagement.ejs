<div class="container pb-5 mb-5">
    <form action="/admin/categorymanagement/" method="post">
        <div class="row col-md-6 px-5 pl-5 ml-5">
            <div class="default-form-box">
                <label><h4>Add New Category<span>*</span></h4></label>
                <input type="text" name="category" placeholder="Enter a category name">
            </div>
            <div class="login_submit">
                <button id="subbtn" class="btn btn-md btn-aqua" type="submit">Add category</button>
            </div>
        </div>
